<%= error_messages_for 'query' %>
<% if @query_logs.valid? %>
    <% if @logs.empty? %>
        <p class="nodata"><%= l(:label_no_data) %></p>
    <% else %>
        <%= render :partial => 'time_logs/list_unbooked_logs', :locals => {:bookings => @logs, :query => @query_logs} %>
        <p class="pagination"><%= pagination_links_full @log_pages, @log_count %></p>
    <% end %>
<% end %>